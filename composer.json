{
  "name": "macellan/turatel",
  "description": "Turatel OTP-Bulk SMS notification for Laravel",
  "homepage": "https://macellan.net",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Arif Demir",
      "email": "cwepicentre@gmail.com",
      "homepage": "https://github.com/epicentre",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Macellan\\Turatel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Macellan\\Turatel\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.5",
    "illuminate/http": "^10.0",
    "illuminate/notifications": "^10.0",
    "illuminate/support": "^10.0",
    "spatie/array-to-xml": "^3.1"
  },
  "require-dev": {
    "orchestra/testbench": "^8.5",
    "phpunit/phpunit": "^10.1"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test:coverage": "vendor/bin/phpunit --coverage-text",
    "test:coverage:html": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Macellan\\Turatel\\TuratelServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
